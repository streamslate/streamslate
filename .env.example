# StreamSlate Environment Configuration Template
# Copy this file to .env and configure for your development environment

# Development settings
NODE_ENV=development
VITE_APP_NAME="StreamSlate"
VITE_APP_VERSION="0.0.1"

# Feature flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_TELEMETRY=false

# API Configuration (when needed)
# VITE_API_BASE_URL=http://localhost:3000
# VITE_API_TIMEOUT=5000

# Logging
VITE_LOG_LEVEL=info

# Build configuration
# TAURI_PRIVATE_KEY=""
# TAURI_KEY_PASSWORD=""